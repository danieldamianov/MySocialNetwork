@using SocialNetwork.Models.Users.Search

@model UsersCollectionSearchViewModel

<ul class="list-group">
    @foreach (var user in Model.Users)
    {
        <li class="table">
            <a href="/Users/Profile/?userId=@user.Id">
                <img style=" border-radius: 50%; width : 50px ; height: 50px" src="@user.ProfilePictureId" alt="Alternate Text" />
                <h3>@user.Name</h3>
            </a>
        </li>
    }
</ul>